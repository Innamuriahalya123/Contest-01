class Solution {
    public int singleNumber(int[] nums) {
        int res = 0;

        for (int i = 0; i < 32; i++) {
            int s = 0;
            int m = 1 << i;
            for (int num : nums) {
                if ((num & m) != 0) {
                    s++;
                }
            }
            if (s % 3 != 0) {
                res |= m;
            }
        }

        return res;
    }
}
